<?xml version="1.0" encoding="UTF-8"?>
<annotations>
  <!-- datatype section under construction. Will not be parsed by anyone/anything-->
  <buildingBlock name="vec16bit" type="bitvector">
    <smt>(_ BitVec 16)</smt>
  </buildingBlock>

  <buildingBlock name="vec8bit" type="bitvector">
    <smt>(_ BitVec 8)</smt>
  </buildingBlock>

  <buildingBlock name="String" type="bitvector">
    <smt>(_ BitVec 32)</smt>
  </buildingBlock>

  <datatype name="IPV6">
    <delimiter>:</delimiter>
    <fields cardinality="8" prefix="o">
      <field>
	<bb>vec16bit</bb>
	<range>
	  <min>#x0000</min>
	  <max>#xFFFF</max>
	</range>
      </field>
    </fields>
  </datatype>
  <datatype name="IPV4">
    <delimiter>.</delimiter>
    <fields cardinality="4" prefix="q">
      <field>
	<bb>vec8bit</bb>
	<range>
	  <min>#x00</min>
	  <max>#xFF</max>
	</range>
      </field>
    </fields>
  </datatype>
  <datatype name="MAC"> 
    <delimiter>:</delimiter>
    <fields cardinality="6" prefix="m">
      <field>
	<bb>vec8bit</bb>
	<range>
	  <min>#x00</min>
	  <max>#xFF</max>
	</range>
      </field>
    </fields>
  </datatype>

<?xml version="1.0" encoding="UTF-8"?>
<annotations>
  <!-- datatype section under construction. Will not be parsed by anyone/anything-->
  <buildingBlock name="vec16bit" type="bitvector">
    <smt>(_ BitVec 16)</smt>
  </buildingBlock>

  <buildingBlock name="vec8bit" type="bitvector">
    <smt>(_ BitVec 8)</smt>
  </buildingBlock>

  <!--<buildingBlock name="String" type="bitvector">
    <smt>(_ BitVec 32)</smt>
  </buildingBlock>-->

  <buildingBlock name="asNumber" type="bitvector">
    <smt>(_ BitVec 16)</smt>
  </buildingBlock>

  <buildingBlock name="Number" type="bitvector">
    <smt>(_ BitVec 32)</smt>
  </buildingBlock>

  <datatype name="IPV6">
    <delimiter>:</delimiter>
    <fields cardinality="8" prefix="o">
      <field>
	<bb>vec16bit</bb>
	<range>
	  <min>#x0000</min>
	  <max>#xFFFF</max>
	</range>
      </field>
    </fields>
  </datatype>
  <datatype name="IPV4">
    <delimiter>.</delimiter>
    <fields cardinality="4" prefix="q">
      <field>
	<bb>vec8bit</bb>
	<range>
	  <min>#x00</min>
	  <max>#xFF</max>
	</range>
      </field>
    </fields>
  </datatype>
  <datatype name="MAC"> 
    <delimiter>:</delimiter>
    <fields cardinality="6" prefix="m">
      <field>
	<bb>vec8bit</bb>
	<range>
	  <min>#x00</min>
	  <max>#xFF</max>
	</range>
      </field>
    </fields>
  </datatype>
  <datatype name="DIST">
    <exclusive>
      <option id="1">
	<delimiter>:</delimiter>
	<field id="asnumber">
	  <bb>asNumber</bb>
	  <range>
	    <min>#x0000</min>
	    <max>#xFFFF</max>
	  </range>
	</field>
	<field id="number">
	  <bb>Number</bb>
	  <range>
	    <min>#x00000000</min>
	    <max>#xFFFFFFFF</max>
	  </range>
	</field>
      </option>

      <option id="2">
	<delimiter>:</delimiter>
	<field id="number">
	  <bb>Number</bb>
	  <range>
	    <min>#x00000000</min>
	    <max>#xFFFFFFFF</max>
	  </range>
	</field>
	<field id="asnumber">
	  <bb>asNumber</bb>
	  <range>
	    <min>#x0000</min>
	    <max>#xFFFF</max>
	  </range>
	</field>
      </option>

      <option id="3">
	<delimiter>:</delimiter>
	<fieldDatatypeRef id="ipv4">IPV4</fieldDatatypeRef>
	<field id="asnumber">
	  <bb>asNumber</bb>
	  <range>
	    <min>#x0000</min>
	    <max>#xFFFF</max>
	  </range>
	</field>
	<field id="ipv4">
	  <dtRef>IPV4</dtRef>
	</field>
      </option>
      
    </exclusive>
  </datatype>
  <!--solver-parameters-->
  <solver>
    <path>/home/ebopaul/Documents/smt/z3-4.3.2.8ef4ec7009ab-x64-debian-7.4/bin/z3</path>
    <arg>-smt2</arg>
  </solver>
  <!--datatype annotations-->
  <dtMapping old="RouterIdStr" new="IPV4">
    <!--<q1>
      <range>
	<min>#x00</min>
	<max>#x7E</max>
      </range>
      <range>
	<min>#x80</min>
	<max>#xDF</max>
      </range>	
    </q1>-->
  </dtMapping>
  <dtMapping old="Ipv4UnicastAddressStr" new="IPV4">
    <q1>
      <range>
	<min>#x00</min>
	<max>#x7E</max>
      </range>
      <range>
	<min>#x80</min>
	<max>#xDF</max>
      </range>
    </q1>
  </dtMapping>
  <dtMapping old="RouteDistinguisher" new="DIST"/>


  <!-- strategy -->
  <strategy>
    <maxCoverage>
      <value>4</value>
      <unit>%</unit>
    </maxCoverage>
    <behavior>
      <attributes>EXPLORE</attributes><!--BOUNDARY-->
    </behavior>
  </strategy>

  <!-- mp.xml to smt mapping -->
  <xml2SMTmapping xml="int16" smt="Int"/>
  <xml2SMTmapping xml="int32" smt="Int"/>
  <xml2SMTmapping xml="int64" smt="Int"/>
  <xml2SMTmapping xml="uint8" smt="Int"/>
  <xml2SMTmapping xml="uint16" smt="Int"/>
  <xml2SMTmapping xml="uint32" smt="Int"/>
  <xml2SMTmapping xml="uint64" smt="Int"/>
  <xml2SMTmapping xml="boolean" smt="Bool"/>
  <xml2SMTmapping xml="string" smt="(_ BitVec 32)"/>
</annotations>

